package com.mrv.technology.ambrack.medical.stock.MediacalStockApplication.Login;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.*;

@Controller
public class LoginController {

	
	
	 @RequestMapping({"","/","/index","/index.html","/login"})
	    public String index()
	    {
	        return "login/login.html";
	    }
	 
	 
	 @PostMapping(path="/checkLogin", produces = "application/json; charset=UTF-8")
	    public String checkAndInsertLoginCredentials()
	    {
		 
		 	
	        return "login/login.html";
	    }
	 
}
